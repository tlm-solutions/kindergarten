{
  "name": "kindergarten",
  "version": "0.0.0",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssl": "ng serve --ssl --host local.tlm.solutions",
    "build": "ng build --output-path ./dist",
    "build:ci": "ng build --output-path ./dist --localize",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "ng lint --fix",
    "lint:ci": "ng lint",
    "extract-i18n": "ng extract-i18n --out-file ./src/locales/en-US.xlf --format xlif"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.1",
    "@angular/common": "^21.0.1",
    "@angular/compiler": "^21.0.1",
    "@angular/core": "^21.0.1",
    "@angular/forms": "^21.0.1",
    "@angular/platform-browser": "^21.0.1",
    "@angular/platform-browser-dynamic": "^21.0.1",
    "@angular/router": "^21.0.1",
    "@feel/form": "^0.2.4",
    "@feel/notification": "^0.2.4",
    "@feel/style": "^0.2.4",
    "@fontsource/dm-sans": "^5.2.8",
    "ol": "10.7.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.1",
    "@angular/localize": "^21.0.1",
    "@types/jasmine": "~5.1.13",
    "jasmine-core": "~5.12.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.3"
  }
}
